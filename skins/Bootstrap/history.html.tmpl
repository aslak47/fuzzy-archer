#include "head.html.inc"
<body>
<!-- Menu navbar -->
#include "nav.html.inc"
<!-- Main container -->
<div class="container-fluid">
    <div class="row">
        <!-- Gauge panel -->
        #include "livegauges.html.inc"
        <!-- Graph panel -->
        <div class="col-md-9 main">
            #for $table in $history_tables
            #if $table.noaa is False
            <h3 class="historyHeader">$table.header_text</h3>
            <table class="table historyTable text-center">
                <thead>
                    <tr>
                        #for $header in $table.headers
                        <th class="head">$header</th>
                        #end for
                    </tr>
                </thead>
                <tbody>
                #for $line in $table.lines
                <tr>
                    <th class="head">$line.head</th>
                    #for $value in $line.values
                    <td class="$value.class">$value.value</td>
                    #end for
                </tr>
                #end for
                </tbody>
            #end if
            #end for
            </table>
        </div>
    </div>
    $history_tables
    #include "foot.html.inc"
</div>
#include "script.html.inc"
</body>
</html>
